<?xml version="1.0" ?>
<launch>
    <arg name="cmd_vel" default="/servo_server/delta_twist_cmds"/>

    <node name="direct_teleop" pkg="constrained_manipulability" type="direct_teleop" output="screen">
        <param name="cmd_pub_topic" value="$(arg cmd_vel)"/>
    </node>

    <!-- This node publishes commands from the controller -->
    <!--node name="teleop_twist_keyboard" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py">
        <param name="speed" value="0.03"/>
        <param name="turn" value="0.014"/>
    </node-->

    <!-- This node does the servoing calculations -->
    <node name="servo_server" pkg="moveit_servo" type="servo_server" output="screen" >
        <param name="parameter_ns" type="string" value="servo_server"/>
        <rosparam command="load" file="$(find moveit_servo)/config/ur_simulated_config.yaml"/>
    </node>
</launch>
